<div class="container-fluid">
  <div class="row-fluid">
      <div id="map-wrap">
        <div id="map">
        </div>
      </div>
  </div>
</div>

<div id="modal-placeholder">
</div>

<div id="loading" class="full-screen-overlay">
  <div class="loading-indicator">
  </div>
</div>

<script type="x-tmpl" id="parcela_template">
  <div id="parcela-modal" class="modal hide fade">
    <div class="modal-header">
      <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
      <h3>Detalles</h3>
    </div>
    <div class="modal-body">
      <fieldset>
        {{ _.each(items,function(item,key,list){ }}
        <dl class="dl-horizontal"><dt>Nombre</dt><dd>{{= item.nombre}}</dd></dl>
        <dl class="dl-horizontal"><dt>Calle</dt><dd>{{= item.calle}} {{= item.nro}}</dd></dl>
        <dl class="dl-horizontal"><dt>Categoria</dt><dd>{{= item.tipo2}}</dd></dl>
        <hr />
        {{ }); }}
      </fieldset>
      <div style="text-align:center;">
        <img class="parcela-image" src="http://fotos.usig.buenosaires.gob.ar//getFoto?smp={{= items[0].smp}}&i=0&w=243&h=195"
             onerror="this.onerror = null; this.src = 'assets/no_photo.png'"/>
      </div>
    </div>
    <div class="modal-footer">
      <button class="btn" data-dismiss="modal" aria-hidden="true">Buscar en los alrededores</button>
    </div>
  </div>

</script>

<script type="x-tmpl" id="no_details_template">
  <h4>No hay detalles para la parcela seleccionada</h4>
</script>

<script type="x-tmpl" id="no_results_template">
  <h4>No hay resultados para la busqueda</h4>
</script>