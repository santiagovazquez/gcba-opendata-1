var Sidebar = function(){

    return {
        displayParcela : function(parcelas){

            if (_(parcelas.properties.parcelas_data).size() > 0) {
                var template = $("#parcela_template").html();
                $("#sidebar").html(_.template(template, {items: parcelas.properties.parcelas_data}));
            }else{
                var template = $("#no_details_template").html();
                $("#sidebar").html(_.template(template));
            }
        }
    };
}();