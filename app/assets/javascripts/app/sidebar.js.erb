var Sidebar = function(){

    return {
        displayParcela : function(parcelas){

            if (_(parcelas.properties.parcelas_data).size() > 0) {
                var template = $("#parcela_template").html();

                var sanitazedItems = _.map(parcelas.properties.parcelas_data, function(item){
                    var newParcela = cloneParcela(item);
                    newParcela.smp = newParcela.smp.substring(1);
                    return newParcela;
                });
                $("#modal-placeholder").html(_.template(template, {items: sanitazedItems}));

                $("#parcela-modal").modal()
            }
        }
    };
}();