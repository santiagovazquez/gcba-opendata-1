#map {
    min-width: 900px;
    min-height: 680px;
}

#map img {
    max-width:none;
}

#map label {
    width: auto; display:inline;
}

#map-wrap { height: 100%; width: 100%;}

.SvgOverlay {
    position: relative;
    width: 500px;
    height: 500px;
}

.SvgOverlay svg {
    position: absolute;
    top: -4000px;
    left: -4000px;
    width: 8000px;
    height: 8000px;
}

.SvgOverlay path {
    stroke: Orange;
    stroke-width: 2px;
    fill: Orange;
    fill-opacity: .3;
}

.SvgOverlay path:hover {
    stroke: #66CCFF;
    fill: #66CCFF;
}
