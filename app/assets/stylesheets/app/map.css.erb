#map {
    min-width: 900px;
    min-height: 700px;
}

#map img {
    max-width:none;
}

#map label {
    width: auto; display:inline;
}

#map-wrap { height: 100%; width: 100%;}

.SvgOverlay {
    position: relative;
    width: 500px;
    height: 500px;
}

.SvgOverlay svg {
    position: absolute;
    top: -4000px;
    left: -4000px;
    width: 8000px;
    height: 8000px;
}

.parcel {
    stroke: Orange;
    stroke-width: 1px;
    fill: Orange;
    fill-opacity: .3;
}

.parcel.espacio-verde {
    stroke: SeaGreen;
    fill: SeaGreen;
}

.parcel:hover {
    stroke: #66CCFF;
    fill: #66CCFF;
    cursor: pointer; cursor: hand;
}

.parcel.clicked {
    stroke: blue;
    fill: blue;
}

.parcel-not-clickeable {
    stroke: Gray;
    stroke-width: 1px;
    fill: Gray;
    fill-opacity: .3;

}
